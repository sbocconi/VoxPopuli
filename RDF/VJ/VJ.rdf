	 MediaClipping:endFrame="00:10:06.11"
	 rdfs:label="ElsbethBarkode_I_02">
	<VoxPopuli_rdfs:Transcript>Daar dacht ik vroeger anders over, ik zou zeggen nee dat hoeft het niet te zijn.</VoxPopuli_rdfs:Transcript>
	<VoxPopuli_rdfs:hasSubject rdf:resource="&VJ_rdf;VJ_Instance_70001"/>
